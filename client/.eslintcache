rue);\n        //return to if user doesn not exist means someone cam here from illegal way \n\n        // emit join user event to server with below params \n        // socket.emit('join', gameId);\n\n        // socket.on('waitingSecondPlayer', ({game}) => {\n        //     setPlayerOne(game?.users?.playerOne)\n        //     setPlayerTwo(game?.users?.playerTwo)\n        //     setWaiting(true)\n        // })\n        // socket.on('startGame' , ({game}) => {\n        //     setPlayerTwo(game?.users?.playerTwo)\n        //     setPlayerOne(game?.users?.playerOne)\n        //     setWaiting(false)\n        //     setPlayNow(true);\n        // })  \n\n        // socket.on('endGame' , () => {\n        //     // setPlayerTwo(null)\n        //     // setPlayerOne(null)\n        //     setNewGame(true)\n        //     setWaiting(false)\n        //     setPlayNow(true);\n        // })  \n        return () => {\n            \n            disconnectSocket();\n          }\n    }, [gameId])    \n\n    \n    //No point in countinuing if user does not exist\n    if (!isLoggedIn()) {\n        return <Redirect to='/login'/>;\n    }\n    return socketHasBeenInitialized && (        \n        <div className='play'>\n            <div>\n                <p>Player one: {playerOne}</p>\n                <p>Player two: {waiting ? 'Waiting...' : playerTwo}</p>\n            </div>\n            <Board gameId={gameId} newGame={newGame} playNow={playNow}/>\n        </div>\n    )\n    // :(\n        // <div><Loading/></div>\n    // )\n}\n\nexport default Play\n","/Users/macbook/apps/tik-tak-toe/Multiplayer-Tic-Tac_toe/client/src/page/home/Home.js",["88"],"/Users/macbook/apps/tik-tak-toe/Multiplayer-Tic-Tac_toe/client/src/page/play/tc-toe Board/Board.js",["89","90","91"],"import React, { useState, useEffect , useRef } from 'react';\nimport Sqaure from './Sqaure'\nimport { useHistory } from 'react-router-dom'\nimport calculateWinner from \"./calculateWinner\";\nimport './Game.css'\nimport { squareClickedReceived, playAgainReceived, closeGame, squareClicked } from '../socket.util'\n\nconst Board = ({ gameId, newGame, playNow }) => {\n    const history = useHistory()\n\tconst username = localStorage.getItem('username')\n\tconst [squares, setSquares] = useState(Array(9).fill(null))\n\tconst [state, updateState] = React.useState();\n\tconst forceUpdate = React.useCallback(() => updateState({}), []);\n\tconst xIsNext = useRef(true);\n\tconst Chance = useRef(1);\n\tconst Player = useRef('');\n\t\n\tconst winner = calculateWinner(squares);\n\tlet status;\n\tif (winner) {\n        console.log('winner:',Player.current)\n\t\t// socket.emit('closeGame', {gameId, winner:Player.current})\n\t\tcloseGame({gameId, winner:Player.current})\n\t\tstatus = (Player.current === username ) ? \n\t\t    'Winner' : 'Looser';\n\t\t// status = 'Better Luck Next Time:' +Player.current;\n\t} else {\n\t\tconsole.log('notwinner:',Player.current)\n\t\tstatus = (Player.current !== username) ? \n\t\t    'Your turn':'Opponent turn';\n\t}\n\n\tuseEffect(() => {\t\n\t\tsquareClickedReceived((err, click) => {\n\t\t\tif(err) {\n\t\t\t\tconsole.log(err)\n\t\t\t\treturn\n\t\t\t}\n\t\t\tconst i = click.i;\t\t\t\n\t\t\tsquares[i] = xIsNext.current ? 'X' : 'O';\n\t\t\txIsNext.current = !xIsNext.current;\t\t\t\t\n\t\t\tsetSquares(squares);\n\t\t\tPlayer.current = click.username;\n\t\t\tif ( Chance.current === 2 ) Chance.current = 1;\n\t\t\tif ( Chance.current === -1 ) Chance.current = 2;\n\t\t\tforceUpdate();\n\t\t})\n\t} , [squares, xIsNext ])\n\n\n\tuseEffect(() => {\n\n\t\tplayAgainReceived((err) => {\n\t\t\tif(err) {\n\t\t\t\tconsole.log(err)\n\t\t\t\treturn \n\t\t\t}\n\t\t\tsquares.fill(null);\n\t\t\tsetSquares(squares)\n\t\t\tChance.current = 1;\n\t\t\tPlayer.current = '';\n\t\t\tforceUpdate();\n\t\t})\n\t}, [squares])\n\t\n\n\tconst handleClick = (i) => {\n\t\tif ( Chance.current === 2 || Chance.current === -1 || calculateWinner(squares) || squares[i]) {\n\t\t\treturn;\n\t\t}\n\n\t\t// console.log('emitting');\n\t\tconst click = {\n\t\t\ti,\n\t\t\tusername,\n\t\t\tgameId\n\t\t};\n\t\t// socket.emit('squareClicked', click);\n\t\tsquareClicked(click)\n\t\tChance.current = -1;\n\t}\n\n\tconst startNewGame = () => {\n\t\thistory.push('/')\n\t}\n\n\tconst renderSquare = (i) => {\n\t\treturn <Sqaure\n\t\t\tval={squares[i]}\n\t\t\tonClick={() => handleClick(i)}\n\t\t/>;\n\t}\n\n\treturn (\n\t\t<div id=\"Board\">\n\t\t\t{ playNow && <div className=\"status\">{status}</div>}\t\t\t\n\t\t\t<div className=\"boardGame\">\n\t\t\t\t<div className=\"board-row\">\n\t\t\t\t\t{renderSquare(0)}\n\t\t\t\t\t{renderSquare(1)}\n\t\t\t\t\t{renderSquare(2)}\n\t\t\t\t</div>\n\t\t\t\t<div className=\"board-row\">\n\t\t\t\t\t{renderSquare(3)}\n\t\t\t\t\t{renderSquare(4)}\n\t\t\t\t\t{renderSquare(5)}\n\t\t\t\t</div>\n\t\t\t\t<div className=\"board-row\">\n\t\t\t\t\t{renderSquare(6)}\n\t\t\t\t\t{renderSquare(7)}\n\t\t\t\t\t{renderSquare(8)}\n\t\t\t\t</div>\n\t\t\t</div>\n\t\t\t<button onClick={startNewGame} className='input-button' disabled={!newGame}>New Game</button>\n\t\t</div>\n\t)\n}\n\nexport default Board\n","/Users/macbook/apps/tik-tak-toe/Multiplayer-Tic-Tac_toe/client/src/page/play/Loading/Loading.js",[],["92","93"],"/Users/macbook/apps/tik-tak-toe/Multiplayer-Tic-Tac_toe/client/src/page/play/tc-toe Board/Sqaure.js",[],"/Users/macbook/apps/tik-tak-toe/Multiplayer-Tic-Tac_toe/client/src/page/play/tc-toe Board/calculateWinner.js",[],"/Users/macbook/apps/tik-tak-toe/Multiplayer-Tic-Tac_toe/client/src/page/auth/Login.js",["94"],"/Users/macbook/apps/tik-tak-toe/Multiplayer-Tic-Tac_toe/client/src/page/auth/Register.js",["95","96"],"/Users/macbook/apps/tik-tak-toe/Multiplayer-Tic-Tac_toe/client/src/auth/index.js",[],"/Users/macbook/apps/tik-tak-toe/Multiplayer-Tic-Tac_toe/client/src/auth/ProtectedRoute.js",["97"],"import React, {Component} from 'react';\nimport {Route, Redirect} from 'react-router-dom';\nimport {isLoggedIn} from './index'\n\n const ProtectedRoute = ({component:Component, 